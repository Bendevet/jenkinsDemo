pipeline{
   agent any
   environment{
   PATH = "/Users/mac/documents/apache-maven-3.6.3/bin"
   }
   stages{
     stage("development"){
       steps{
         git credentialsId: 'bendevet-demo-jenkins', url: 'https://github.com/Bendevet/jenkinsDemo.git'
       }
     }
     stage('build') {
            steps {
                sh 'mvn clean package'
            }
        }
   }
}
