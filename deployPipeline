pipeline{
   agent any
   environment{
     PATH= "/usr/local/opt/MAVEN_HOME"
   }
   stages{
     stage("development"){
       steps{
         git credentialsId: 'bendevet-demo-jenkins', url: 'https://github.com/Bendevet/jenkinsDemo.git'
       }
     }
     stage('build') {
            steps {
                sh "mvn clean package"
            }
        }
   }
}
